#!/usr/bin/env php
<?php
/**
 * This file is a part of the webunit package.
 *
 * Read more at https://github.com/themichaelhall/webunit
 */
declare(strict_types=1);

use MichaelHall\PageFetcher\PageFetcher;
use MichaelHall\Webunit\Application\ConsoleApplication;

foreach ([__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php'] as $autoload) {
    if (file_exists($autoload)) {
        /** @noinspection PhpIncludeInspection */
        require $autoload;

        break;
    }
}

$application = new ConsoleApplication($argc, $argv, new PageFetcher());

return $application->run();
