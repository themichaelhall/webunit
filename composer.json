{
  "name": "michaelhall/webunit",
  "description": "Webunit test client",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "web",
    "unit"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "michaelhall/http-client": "^2.0",
    "datatypes/datatypes": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MichaelHall\\Webunit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelHall\\Webunit\\Tests\\": "tests"
    }
  },
  "bin": [
    "bin/webunit"
  ]
}
