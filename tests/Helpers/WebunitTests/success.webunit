set                BaseUrl = https://example.com/

get                {{ BaseUrl }}
assert-equals      Hello World!
assert-empty!

get                {{ BaseUrl }}foo
assert-contains    Foo
assert-contains!^  Bar
assert-equals^~    .*foo.*
assert-header      x-foo: X-Bar

set-default        EmptyPath = empty

get                {{ BaseUrl }}{{ EmptyPath}}
assert-empty

get                {{ BaseUrl }}not-found
assert-status-code 404
assert-equals      Page not found.

put                {{ BaseUrl }}method
assert-contains^   put
assert-contains!^  get
